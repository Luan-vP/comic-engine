{
  "name": "comic-engine",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src/",
    "format": "prettier --write 'src/**/*.{js,jsx}' '*.{js,json,md}'",
    "format:check": "prettier --check 'src/**/*.{js,jsx}' '*.{js,json,md}'",
    "prepare": "husky"
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@sentry/react": "^10.39.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^9.39.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "globals": "^17.3.0",
    "husky": "^9.1.7",
    "jscpd": "^4.0.8",
    "jsdom": "^28.1.0",
    "knip": "^5.83.1",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.1",
    "vite": "^5.1.0",
    "vitest": "^4.0.18"
  }
}
